{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true,
    "codeceptjs/codeceptjs": true
  },
  "extends": ["airbnb-base", "plugin:codeceptjs/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "operator-linebreak": [
      "error",
      "after",
      { "overrides": { "?": "before", ":": "before" } }
    ],
    "import/no-extraneous-dependencies": 0,
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  },
  "plugins": ["jasmine", "codeceptjs"]
}
